{
  "name": "pwa-todo-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.es.js",
  "exports": {
    ".": "./dist/index.es.js"
  },
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "vite --port 9999",
    "build:vite": "tsc && vite build",
    "serve": "npm run build && vite preview",
    "esbuild": "esbuild --outfile=sw.js --bundle sw.ts",
    "workbox:build": "workbox injectManifest workbox-config.cjs",
    "build": "npm run build:vite && npm run esbuild && npm run workbox:build"
  },
  "dependencies": {
    "lit": "^2.3.1",
    "mobx": "^6.6.2",
    "netlify-cli": "^12.0.7"
  },
  "devDependencies": {
    "esbuild": "^0.15.10",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "workbox-cli": "^6.5.4",
    "workbox-precaching": "^6.5.4"
  }
}
